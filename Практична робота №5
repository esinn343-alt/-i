class User {
private String name;
private int age;
private static int userCount = 0; //
статичне поле
public User(String name, int age) {
this.name = name;
this.age = age;
userCount++;
System.out.println("[User] Створено користувача: " + name);

}
public void printInfo() {
System.out.println(" Iм'я: " + name + ", Вiк: " +age);

}

public static int getCount() {
return userCount;

}

